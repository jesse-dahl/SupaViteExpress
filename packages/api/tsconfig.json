{
  "extends": "../../tsconfig.json",
  "main": "./index.ts",
  "compilerOptions": {
    "outDir": "dist",
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noEmit": false,
  },
  "include": ["index.ts", "/*.ts", "src/*.ts"],
  "exclude": [
    "./dist/**/*",
    "./node_modules/**/*",
  ]
}